    "license": "BSD-3-Clause",
    "version": "3.0.0",
    "description": "PXBlue themes for Eaton applications",
    "main": "dist/index.js",
    "scripts": {
        "test": "./buildTest.sh",
        "build:index": "yarn && tsc",
        "build:react": "cd react && yarn build && cd ..",
        "build:react-native": "cd react-native && yarn build && cd ..",
        "build:all": "yarn build:react && yarn build:react-native && yarn build:index",
        "publish:full": "yarn build:all && npm version patch && npm publish",
        "link:angular": "cd angular && yarn link:themes",
        "start:angular": "cd angular && yarn start",
        "link:react": "cd react && yarn link:themes",
        "start:react": "cd react && yarn start",
        "link:react-native": "cd react && yarn link:themes",
        "start:react-native": "cd react && yarn start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pxblue/themes.git"
    },
    "keywords": [
        "PXBlue",
        "themes",
        "Eaton",
        "Power",
        "Xpert",
        "quality",
        "lighting"
    ],
    "bugs": {
        "url": "https://github.com/pxblue/themes/issues"
    },
    "homepage": "https://github.com/pxblue/themes#readme",
    "dependencies": {
        "@pxblue/colors": "^2.0.0",
        "typeface-open-sans": "^0.0.54"
    },
    "devDependencies": {
        "typescript": "^3.7.2"
    },
    "files": [
        "package.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        "dist/index.js",
        "angular/Readme.md",
        "angular/*.scss",
        "react/Readme.md",
        "react/dist",
        "react-native/Readme.md",
        "react-native/dist"
    ]
