{
    "name": "@pxblue/themes",
    "author": "PX Blue <pxblue@eaton.com>",
    "license": "BSD-3-Clause",
    "version": "0.0.0",
    "description": "PXBlue themes for Eaton applications",
    "scripts": {
        "test": "bash ./buildTest.sh",
        "build:all": "yarn build:react && yarn build:react-native",
        "build:react": "cd react && yarn build && cd ..",
        "build:react-native": "cd react-native && yarn build && cd ..",
        "start:angular": "cd angular && yarn start",
        "start:react": "cd react && yarn start",
        "start:react-native": "cd react-native && yarn start",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --write",
        "prettier:check": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --check",
        "update:submodule": "git submodule update --remote"
    },
    "prettier": "@pxblue/prettier-config",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pxblue/themes.git"
    },
    "keywords": [
        "PXBlue",
        "themes",
        "Eaton",
        "Power",
        "Xpert"
    ],
    "bugs": {
        "url": "https://github.com/pxblue/themes/issues"
    },
    "homepage": "https://github.com/pxblue/themes#readme",
    "devDependencies": {
        "@pxblue/prettier-config": "^1.0.2",
        "prettier": "^2.0.2"
    },
    "files": [
        "package.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ]
}
